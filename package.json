{
  "name": "@baseswapfi/default-token-list",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Token list for Baseswap",
  "module": "dist/default-token-list.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepublishOnly": "tsdx build"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "husky": "^8.0.3",
    "size-limit": "^8.2.6",
    "tsdx": "^0.14.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/default-token-list.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/default-token-list.esm.js",
      "limit": "10 KB"
    }
  ]
}
